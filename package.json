{
  "name": "dbinfoz",
  "version": "0.3.0",
  "description": "Get Database Info and Table Schemas",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build:cjs": "babel src --out-dir dist/cjs",
    "build:esm": "babel src --out-dir dist/esm --env-name esm",
    "build": "npm run build:cjs && npm run build:esm",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rootedbox/dbinfoz.git"
  },
  "keywords": [
    "Database",
    "Table",
    "Schema",
    "SQLite",
    "MySQL",
    "Postgres"
  ],
  "author": "Jason Jacobs",
  "license": "MIT",
  "dependencies": {
    "mysql2": "^3.9.1",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7"
  }
}
